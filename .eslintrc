{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    ],
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "space-before-function-paren": [ 2, "never" ],
    "semi": [0],
    "no-console": [0]
  },
  "settings": {
    "import/resolver": "webpack",
    "import/extensions": [".js", ".jsx"]
  }
}
